<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cdsf.locman.yzy.offlinemap">

    <application
        android:name="com.cdsf.locman.yzy.offlinemap.OfflineMapApplication"
        android:allowBackup="true"
        android:icon="@drawable/offlinemap_ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
       >
        <activity
            android:name="com.cdsf.locman.yzy.offlinemap.activity.OfflineMapManageActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
